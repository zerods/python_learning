# Q10
def fib(n):
    """Returns the nth Fibonacci number.

    >>> fib(0)
    0
    >>> fib(1)
    1
    >>> fib(2)
    1
    >>> fib(3)
    2
    >>> fib(4)
    3
    >>> fib(5)
    5
    >>> fib(6)
    8
    """
    if n == 0:
        return 0
    pre, curr = 0, 1
    i  = 1
    while i < n:
        pre, curr = curr, pre + curr
        i += 1
    return curr
